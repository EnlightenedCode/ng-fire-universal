{
  "name": "ng-fire-universal",
  "version": "0.9.3",
  "repository": "https://github.com/jrodl3r/ng-fire-universal",
  "author": "John Rodler",
  "license": "MIT",
  "engines": {
    "node": "10.x.x"
  },
  "scripts": {
    "ng": "ng",
    "dev": "ng serve",
    "build": "ng build",
    "build:ssr": "npm run build:bundles && npm run compile:server",
    "build:bundles": "ng build --prod && ng run ng-fire-universal:server:production",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "start": "npm run serve:ssr",
    "test": "ng test",
    "test:watch": "ng test --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "gcloud app deploy",
    "deploy:functions": "firebase deploy --only functions",
    "init": "npm i && npm i -g firebase-tools && cd functions && npm i && cd ..",
    "preview": "npm run build:ssr && npm run start",
    "ship": "npm run build:ssr && npm run deploy",
    "ship:all": "npm run build:ssr && npm run deploy && npm run deploy:functions"
  },
  "dependencies": {
    "@angular/animations": "~8.2.11",
    "@angular/common": "~8.2.11",
    "@angular/compiler": "~8.2.11",
    "@angular/core": "~8.2.11",
    "@angular/fire": "^5.2.1",
    "@angular/forms": "~8.2.11",
    "@angular/platform-browser": "~8.2.11",
    "@angular/platform-browser-dynamic": "~8.2.11",
    "@angular/platform-server": "~8.2.11",
    "@angular/pwa": "^0.803.12",
    "@angular/router": "~8.2.11",
    "@angular/service-worker": "~8.2.11",
    "@nguniversal/express-engine": "^8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "v8.1.1",
    "express": "^4.17.1",
    "firebase": "^7.2.1",
    "ngx-toastr": "^11.1.2",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "^8.2.0",
    "@angular-devkit/build-angular": "~0.803.12",
    "@angular/cli": "~8.3.12",
    "@angular/compiler-cli": "~8.2.11",
    "@angular/language-service": "~8.2.11",
    "@types/jasmine": "^3.4.4",
    "@types/jasminewd2": "^2.0.8",
    "@types/jest": "^24.0.19",
    "@types/node": "~12.11.1",
    "bufferutil": "^4.0.1",
    "codelyzer": "^5.1.2",
    "first-input-delay": "^0.1.3",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jest": "^24.9.0",
    "protractor": "~5.4.2",
    "ts-loader": "^6.2.0",
    "ts-node": "~8.4.1",
    "tslint": "~5.20.0",
    "typescript": "^3.5.3",
    "utf-8-validate": "^5.0.2",
    "webpack-cli": "^3.3.9",
    "ws": "^7.1.2",
    "xhr2": "^0.2.0"
  }
}
